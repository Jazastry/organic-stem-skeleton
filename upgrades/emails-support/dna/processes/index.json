{
  "plasma": {
    "organic-emailrender": {
      "source": "node_modules/organic-emailrender",
      "root": "context/emails",
      "templateEngine": "context/initializers/email-templates",
      "cache": true,
      "reactOn": "sendEmail",
      "emitEmail": "deliverEmail"
    },
    "organic-emailsender": {
      "source": "node_modules/organic-emailsender",
      "email": {
        "transport": "devnull"
      },
      "reactOn": "deliverEmail",
      "from": "noreply@stem-skeleton"
    }
  }
}
