{ 
  "type": "build",
  "source": "node_modules/organic-globdir",
  "root": "context/client",
  "pattern": "/**/*.bundle.js",
  "forEachEmit": {
    "type": "build",
    "source": "node_modules/organic-spawnfilecmd",
    "dest": "build/js",
    "transformations": "@browserify",
    "cmd": "node ./node_modules/.bin/browserify {transformations} {srcfile} | node ./node_modules/.bin/uglifyjs > {destfile}"
  }
}